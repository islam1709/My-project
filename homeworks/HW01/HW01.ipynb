# Импорт библиотеки NumPy
import numpy as np
# Создание массива из списка
a = np.array([10, 20, 30, 40])
print("a:", a)

# Создание массива через arange
b = np.arange(5, 25, 5)   # [5, 10, 15, 20]
print("b:", b)

# Создание массива из нулей и единиц
z = np.zeros(5)
o = np.ones((3, 2))

print("z:", z)
print("o:\n", o)
# Создание двумерного массива (матрицы)
matrix = np.array([
    [2, 4, 6],
    [8, 10, 12],
    [14, 16, 18]
])

print("matrix:\n", matrix)
# Вывод формы и типа данных
print("Форма массива:", matrix.shape)
print("Тип данных:", matrix.dtype)
# Получение одного элемента
element = matrix[1, 2]   # 10
print("matrix[1,2]:", element)

# Получение первой строки
row = matrix[0]
print("Первая строка:", row)

# Получение второго столбца
column = matrix[:, 1]
print("Второй столбец:", column)

# Подмассив (первые две строки и два столбца)
sub_matrix = matrix[:2, :2]
print("Подмассив:\n", sub_matrix)
# Поэлементные операции
x = np.array([3, 6, 9])
y = np.array([2, 4, 6])

sum_result = x + y
mul_result = x * y

print("x + y =", sum_result)
print("x * y =", mul_result)
# Статистические вычисления
print("Среднее значение x:", x.mean())
print("Среднее значение y:", y.mean())

print("Сумма элементов matrix:", matrix.sum())
print("Среднее по строкам matrix:", matrix.mean(axis=1))
print("Среднее по столбцам matrix:", matrix.mean(axis=0))
